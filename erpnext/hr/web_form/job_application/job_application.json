lue(\"contact_no\", '0')\n                frappe.throw(\"Kindly Check the Contact Number\");\n        }\n        }\n\t})\n\t\nfrappe.web_form.on('aadhar', (field, value) => {\n        var fields = value;\n\t\tvar mxlen = 12;\n\t\tvar mnlen = 12;\n\t\tif (fields != 0){\n        var pattern1 = /^([0-9]+(\\s+)?(\\-(\\s+)?[0-9]+)*,*)+$/gm;\n        if (!pattern1.test(value))\n                {frappe.web_form.set_value(\"aadhar\", '0')\n                frappe.throw('Kindly Check the Aadhar Card Number');\n                return false;\n\t\t}\n        if(fields.length > mxlen || fields.length < mnlen){ \n\t\t        frappe.web_form.set_value(\"aadhar\", '0')\n                frappe.throw(\"Kindly Check the Aadhar Number\");\n        }\n        }\n\t})\n\t\nfrappe.web_form.on('pin_code', (field, value) => {\n        var fields = value;\n\t\tvar mxlen = 6;\n\t\tvar mnlen = 6;\n\t\tif (fields != 0){\n        var pattern1 = /^([0-9]+(\\s+)?(\\-(\\s+)?[0-9]+)*,*)+$/gm;\n        if (!pattern1.test(value))\n                {frappe.web_form.set_value(\"pin_code\", '0')\n                frappe.throw('Kindly Check the Pin Code');\n                return false;\n\t\t}\n        if(fields.length > mxlen || fields.length < mnlen){ \n\t\t        frappe.web_form.set_value(\"pin_code\", '0')\n                frappe.throw(\"Kindly Check the Pin Code\");\n        }\n        }\n\t})\n\nfrappe.web_form.on('email_id', (field, value) => {\n    if (value){\n    if (/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(value))\n  {\n    return (true)\n  }\n    else{\n        frappe.web_form.set_value(\"email_id\", '')\n        frappe.throw(\"You have entered an invalid email address!\")\n\n}\n}\n})\nvar seconds = 0, minutes = 0, hours = 0,t;\nfunction timer() {\n    t = setTimeout(add, 1000);\n}\nfunction add() {\n    seconds++;\n    if (seconds >= 60) {\n        seconds = 0;\n        minutes++;\n        if (minutes >= 60) {\n            minutes = 0;\n            hours++;\n        }\n    }\n    var h1 = (hours ? (hours > 9 ? hours : \"0\" + hours) : \"00\") + \":\" + (minutes ? (minutes > 9 ? minutes : \"0\" + minutes) : \"00\") + \":\" + (seconds > 9 ? seconds : \"0\" + seconds);\n    console.log(h1);\n    frappe.web_form.set_value(\"timing\", h1)\n    if (h1==\"00:00:10\"){\n    frappe.web_form.save();\n    }\n    timer();\n}\n\n\n\nfrappe.web_form.on('start_test', (field, value) => {\n\t\tvar starttime=value;\n\t\tconsole.log(starttime);\n\t\tif (starttime==\"Yes\"){\n\t\tfrappe.web_form.set_df_property(\"email_id\",\"read_only\",1);\n\t\tfrappe.web_form.set_df_property(\"applicant_name\",\"read_only\",1);\n\t\tfrappe.web_form.set_df_property(\"start_test\",\"read_only\",1);\n\t\tconsole.log(\"yes\");\n        timer();\n        console.log(\"done\");\n\t\t}\n\t})\n\n",
